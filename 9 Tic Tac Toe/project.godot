config_version=5

[application]

config/name="9 Tic Tac Toe"
run/main_scene="res://Scenes/Menues/title.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DataPaths="*res://Scenes/Battle/Variables/data_paths.tscn"
GUI="*res://Scenes/Battle/GUI/gui.tscn"
Variables="*res://Scenes/Battle/Variables/variables.tscn"
Audio="*res://Scenes/Battle/Audio/audio.tscn"

[display]

window/stretch/mode="viewport"
window/stretch/aspect="ignore"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/default_filters/anisotropic_filtering_level=0
